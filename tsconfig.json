{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "ES6",
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "strictNullChecks": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "baseUrl": ".",
    "paths": {
      "@/container/*": [
        "./src/container/*"
      ],
      "@/components/*": [
        "./src/components/*"
      ],
      "@/hooks/*": [
        "./src/hooks/*"
      ],
      "@/layout/*": [
        "./src/layout/*"
      ],
      "@/styles/*": [
        "./src/styles/*"
      ],
      "@/context/*": [
        "./src/context/*"
      ],
      "@/utils/*": [
        "./src/context/*"
      ],
    }
  },
  "include": [
    "src"
  ],
  "exclude": [
    "node_modules"
  ]
}